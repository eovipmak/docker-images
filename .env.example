# Database Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=v_insight

# Backend API Configuration
BACKEND_PORT=8080
# CORS_ALLOWED_ORIGINS - Comma-separated list of allowed origins
# For development: http://localhost:3000,http://127.0.0.1:3000
# For production: Set to your exact frontend domain (e.g., https://yourdomain.com)
# NEVER use "*" in production
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Worker Configuration
WORKER_PORT=8081

# Frontend Configuration
FRONTEND_PORT=3000
# PUBLIC_API_URL - API endpoint accessible from browser
# For local development: http://localhost:8080
# For production: Set to your backend domain (e.g., https://api.yourdomain.com)
PUBLIC_API_URL=http://localhost:8080

# Environment
ENV=development
