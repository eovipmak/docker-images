{
  "name": "v-insight-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "PLAYWRIGHT_HTML_REPORT_OPEN=never playwright test",
    "test:e2e:ci": "start-server-and-test 'npm run dev' http://localhost:3000 'npm run test:e2e'",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "postinstall": "[ -f setup.js ] && node setup.js || true",
    "setup": "node setup.js",
    "analyze": "ANALYZE=true vite build"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-node": "^5.4.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/svelte": "^5.2.6",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.22",
    "happy-dom": "^15.11.7",
    "postcss": "^8.5.6",
    "rollup-plugin-visualizer": "^5.12.0",
    "start-server-and-test": "^2.0.5",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.18",
    "terser": "^5.31.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vitest": "^2.1.8"
  },
  "type": "module",
  "dependencies": {
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0"
  }
}
